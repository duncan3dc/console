{
    "name": "duncan3dc/console",
    "type": "library",
    "description": "Create command line php applications using symfony/console",
    "keywords": ["console", "cli", "symfony"],
    "homepage": "https://github.com/duncan3dc/console",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Craig Duncan",
        "email": "git@duncanc.co.uk",
        "homepage": "https://github.com/duncan3dc",
        "role": "Developer"
    }],
    "require": {
        "duncan3dc/symfony-climate": "^0.3",
        "nunomaduro/collision": "^2.1",
        "stecman/symfony-console-completion": "^0.9",
        "symfony/console": "^3.4|^4.2",
        "symfony/event-dispatcher": "^3.4|^4.2",
        "symfony/filesystem": "^3.4|^4.2",
        "symfony/finder": "^3.4|^4.2",
        "php": "^7.1"
    },
    "conflict": {
        "symfony/console": "3.4.16|4.1.5"
    },
    "require-dev": {
        "maglnet/composer-require-checker": "^1.0",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {"duncan3dc\\Console\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"duncan3dc\\ConsoleTests\\": "tests/"}
    }
}
