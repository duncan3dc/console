{
    "name": "duncan3dc/console",
    "description": "Create command line php applications using symfony/console",
    "require": {
        "duncan3dc/symfony-climate": "^0.1.0",
        "stecman/symfony-console-completion": "^0.7.0",
        "symfony/console": "^2.7|^3.0|^4.0",
        "symfony/event-dispatcher": "^2.7|^3.0|^4.0",
        "symfony/filesystem": "^2.7|^3.0|^4.0",
        "symfony/finder": "^2.7|^3.0|^4.0",
        "php": "^5.6|^7.0"
    },
    "require-dev": {
        "maglnet/composer-require-checker": "^0.1.6",
        "mockery/mockery": "^1.0",
        "php-coveralls/php-coveralls": "^2.0",
        "phpunit/phpunit": "^5.7.1"
    },
    "autoload": {
        "psr-4": {"duncan3dc\\Console\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"duncan3dc\\ConsoleTests\\": "tests/"}
    }
}
